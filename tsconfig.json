{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./out",
    "rootDir": "./",
    "removeComments": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@App/*": ["./src/app/*"],
      "@Lib/*": ["./src/lib/*"],
      "@/*": ["./src/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
